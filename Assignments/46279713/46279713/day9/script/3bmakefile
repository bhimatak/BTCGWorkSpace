CC= gcc

SRC= src
INC= inc
BIN =bin
OBj= obj

all: $(BIN)/app1

SetUp:
	mkdir $(OBJ) $(BIN)

$(BIN)/app1: $(OBJ)/3b.o
	$(CC) -o $(BIN)/app1 $(OBJ)/3b.o

$(OBJ)/3b.o: $(SRC)/3b.c
	$(CC) -c $(SRC)/3b.c -I $(INC)/
	mv 3b.o $(OBJ)/

clean:
	rm -f a.out *.o
	rm -f $(OBJ)/*
	rm -f $(BIN)/*
	@echo "Cleaning Done!"
